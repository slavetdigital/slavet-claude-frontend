{
  "name": "slavet-claude-frontend",
  "version": "0.1.0",
  "description": "Enterprise frontend assistant with tokens & contrast checks, resource caps, and sub-agent orchestration.",
  "entrypoints": {
    "commands": "../commands/index.js",
    "agents": "../agents/index.js",
    "hooks": "../hooks/index.js"
  },
  "config": {
    "tokenCaps": {
      "prompt": 4000,
      "completion": 1000,
      "parallel": 2,
      "session": 100000
    },
    "strictEvidence": true,
    "backgroundAudits": {
      "enabled": true,
      "maxDailyTokens": 15000
    },
    "interop": {
      "detectSlavetPlugins": true,
      "coordinateSubAgents": true
    }
  }
}
